{
	"compileOnSave": false,
	"compilerOptions": {
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"module": "system",
		"moduleResolution": "node",
		"noEmitOnError": false,
		"noImplicitAny": false,
		"removeComments": false,
		"target": "es6"
	},
	"exclude": [
		"node_modules",
		"wwwroot"
	]
}